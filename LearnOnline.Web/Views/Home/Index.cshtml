    function inviteByEmail() {
        var friendEmail = $("input[name='friendEmail']").val();
        var friendName = $("input[name='friendName']").val();
        if (friendName.length <= 0) {
            $("input[name='friendName']").addClass("error");
            return;
        }
        if (!checkValidEmailAddress(friendEmail)) {
            $("input[name='friendEmail']").addClass("error");
            return;
        }
        var url = ROOT_URL + "User/InviteFriendByEmail?email=" + encodeURIComponent(friendEmail) + "&name=" + encodeURIComponent(friendName);
        $.getJSON(url, function (data) {
            if (data) {
                $("input[name='friendEmail']").val('');
                $("input[name='friendName']").val('');

            }
            $.fancybox.open("#completeInvite");
        });
    }
